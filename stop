[32m+[m[32m    /*M√©todo auxiliar para asuntos de resoluci√≥n*/[m
[32m+[m[32m    private void gestionarResolucion() {[m
[32m+[m
[32m+[m[32m        //independencia de la resoluci√≥n de la pantalla[m
[32m+[m[32m        DisplayMetrics displayMetrics = this.getApplicationContext().getResources().getDisplayMetrics();[m
[32m+[m[32m        int alto = displayMetrics.heightPixels;[m
[32m+[m[32m        int ancho = displayMetrics.widthPixels;[m
[32m+[m[32m        int dimensionReferencia;[m
[32m+[m
[32m+[m[32m        //tomar el menor valor entre alto y ancho de pantalla[m
[32m+[m[32m        if (alto > ancho) {[m
[32m+[m
[32m+[m[32m            dimensionReferencia = ancho;[m
[32m+[m[32m        } else {[m
[32m+[m
[32m+[m[32m            dimensionReferencia = alto;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m        //una estimaci√≥n de un buen tama√±o[m
[32m+[m[32m        int tamanoLetra = dimensionReferencia / 20;[m
[32m+[m
[32m+[m
[32m+[m[32m        //tamano de letra para usar acomodado a la resoluci√≥n de pantalla[m
[32m+[m[32m        tamanoLetraResolucionIncluida = (int) (tamanoLetra / displayMetrics.scaledDensity);[m
[32m+[m
[32m+[m[32m        //guardar en el almacen de datos para que otras clases la accedan f√°cilmente[m
[32m+[m[32m        AlmacenDatosRAM.tamanoLetraResolucionIncluida = tamanoLetraResolucionIncluida;[m
[32m+[m
[32m+[m[32m        //guardar en almacen annho y alto de pantalla:[m
[32m+[m[32m        //este c√°lculo est√° con dispoitivo m√≥vil[m
[32m+[m[32m        //en orientaci√≥n portrait[m
[32m+[m[32m        AlmacenDatosRAM.ancho_pantalla =ancho;[m
[32m+[m[32m        AlmacenDatosRAM.alto_pantalla =alto;[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
