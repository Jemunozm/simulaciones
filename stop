[32m+[m[32m        textVelocidad.setText(marca_velocidad);[m
[32m+[m
[32m+[m[32m        textAngulo = new TextView(this);[m
[32m+[m[32m        textAngulo.setTextSize(TypedValue.COMPLEX_UNIT_SP, tamanoLetraResolucionIncluida);[m
[32m+[m[32m        textAngulo.setGravity(Gravity.CENTER);[m
[32m+[m[32m        textAngulo.setTextColor(Color.BLACK);[m
[32m+[m[32m        String marca_angulo = "ANGULO gra. \n -90 a +90 ";//con salto de l√≠nea[m
[32m+[m[32m        textAngulo.setText(marca_angulo);[m
[32m+[m
[32m+[m[32m        seekBarAceleracion = new SeekBar(this);[m
[32m+[m[32m        seekBarAceleracion.setMax(40);[m
[32m+[m[32m        seekBarAceleracion.setProgress(10);//aceleraci√≥n inicial -10[m
[32m+[m[32m        seekBarAceleracion.setScaleY(0.2f);[m
[32m+[m[32m        aceleracion = seekBarAceleracion.getProgress() - 20;[m
[32m+[m[32m        AlmacenDatosRAM.aceleracion=aceleracion;[m
[32m+[m
[32m+[m[32m        seekarVelocidadInicial = new SeekBar(this);[m
[32m+[m[32m        seekarVelocidadInicial.setMax(30);[m
[32m+[m[32m        seekarVelocidadInicial.setProgress(20);//velocidad inicial 20[m
[32m+[m[32m        seekarVelocidadInicial.setScaleY(0.2f);[m
[32m+[m[32m        velocidadInicial = seekarVelocidadInicial.getProgress();[m
[32m+[m[32m        AlmacenDatosRAM.vInicial=velocidadInicial;[m
[32m+[m[32m        AlmacenDatosRAM.velocidad=velocidadInicial;[m
[32m+[m
[32m+[m[32m        seekBarAngulo = new SeekBar(this);[m
[32m+[m[32m        seekBarAngulo.setMax(180);[m
[32m+[m[32m        seekBarAngulo.setProgress(150);//√°ngulo inicial -30[m
[32m+[m[32m        seekBarAngulo.setScaleY(0.2f);[m
[32m+[m[32m        angulo = seekBarAngulo.getProgress()-90;[m
[32m+[m[32m        AlmacenDatosRAM.angulo = angulo;[m
[32m+[m
[32m+[m
[32m+[m[32m        botonEmpezar = new Button(this);[m
[32m+[m[32m        botonEmpezar.setTextSize(TypedValue.COMPLEX_UNIT_SP, tamanoLetraResolucionIncluida);[m
[32m+[m[32m        botonEmpezar.setText("EMPEZAR");[m
[32m+[m[32m        botonEmpezar.getBackground().setColorFilter(Color.argb(180, 250, 170, 50), PorterDuff.Mode.MULTIPLY);[m
[32m+[m
[32m+[m[32m        botonPausar = new Button(this);[m
[32m+[m[32m        botonPausar.setTextSize(TypedValue.COMPLEX_UNIT_SP, tamanoLetraResolucionIncluida);[m
[32m+[m[32m        botonPausar.setText("PAUSAR");[m
[32m+[m[32m        botonPausar.getBackground().setColorFilter(Color.argb(180, 250, 170, 50), PorterDuff.Mode.MULTIPLY);[m
[32m+[m[32m        botonPausar.setEnabled(false);[m
[32m+[m
[32m+[m
[32m+[m[32m        //creaci√≥n del objeto Pizarra[m
[32m+[m[32m        pizarra = new Pizarra(this);[m
[32m+[m[32m        //color del tablero[m
[32m+[m[32m        pizarra.setBackgroundColor(Color.WHITE);[m
[32m+[m[32m        pizarra.setSistemaCoordenadas(CR.pcApxX(40f),CR.pcApxY(60f),1, -1);[m
[32m+[m
[32m+[m[32m        crearObjetosLaboratorio();[m
[32m+[m
[32m+[m[32m        estadoInicial();[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /*m√©todo responsable de administrar el dise√±o de la GUI*/[m
[32m+[m[32m    private LinearLayout crearGUI() {[m
[32m+[m
[32m+[m[32m        LinearLayout linearPrincipal = new LinearLayout(this);[m
[32m+[m[32m        linearPrincipal.setOrientation(LinearLayout.HORIZONTAL);[m
[32m+[m[32m        linearPrincipal.setBackgroundColor(Color.BLACK);[m
[32m+[m[32m        linearPrincipal.setWeightSum(10.0f);[m
[32m+[m
[32m+[m
[32m+[m[32m        //linear secudario izquierda[m
[32m+[m[32m        LinearLayout linearIzquierda = new LinearLayout(this);[m
[32m+[m[32m        //linearIzquierda.setWeightSum(1.0f);[m
[32m+[m
[32m+[m
[32m+[m[32m        //linear secudario derecha[m
[32m+[m[32m        LinearLayout linearDerecha = new LinearLayout(this);[m
[32m+[m[32m        linearDerecha.setOrientation(LinearLayout.VERTICAL);[m
[32m+[m[32m        linearDerecha.setBackgroundColor(Color.YELLOW);[m
[32m+[m[32m        linearDerecha.setWeightSum(8.0f);[m
[32m+[m
[32m+[m
[32m+[m[32m        //pegar estos secundarios al principal[m
[32m+[m[32m        LinearLayout.LayoutParams parametrosPegadoIzquierda = new LinearLayout.LayoutParams(0, android.view.ViewGroup.LayoutParams.MATCH_PARENT);[m
[32m+[m[32m        //parametrosPegadoIzquierda.setMargins(0, 5, 0, 5);[m
[32m+[m[32m        //ocupar√° el 80% de linear_principal[m
[32m+[m[32m        parametrosPegadoIzquierda.weight = 8.5f;[m
[32m+[m[32m        linearPrincipal.addView(linearIzquierda, parametrosPegadoIzquierda);[m
[32m+[m
[32m+[m[32m        //pegar la pizarra al linearIzquierdo[m
[32m+[m[32m        //linearIzquierda.setOrientation(LinearLayout.VERTICAL);[m
[32m+[m[32m        linearIzquierda.addView(pizarra);[m
[32m+[m
[32m+[m
[32m+[m[32m        //pegar estos secundarios al princial[m
[32m+[m[32m        LinearLayout.LayoutParams parametrosPegadoDerecha = new LinearLayout.LayoutParams(0, android.view.ViewGroup.LayoutParams.MATCH_PARENT);[m
[32m+[m[32m        //ocupar√° el 20% de linear_principal[m
[32m+[m[32m        parametrosPegadoDerecha.setMargins(0, 5, 0, 5);[m
[32m+[m[32m        parametrosPegadoDerecha.weight = 1.5f;[m
[32m+[m[32m        linearPrincipal.addView(linearDerecha, parametrosPegadoDerecha);[m
[32m+[m
[32m+[m
[32m+[m[32m        //pegar componentes a lineardDerecha.[m
[32m+[m[32m        linearDerecha.setOrientation(LinearLayout.VERTICAL);[m
[32m+[m[32m        LinearLayout.LayoutParams parametrosPegadoComponentes = new LinearLayout.LayoutParams(android.view.ViewGroup.LayoutParams.MATCH_PARENT, 0);[m
[32m+[m[32m        //ocupar√° el 10% de linear_principal[m
[32m+[m[32m        parametrosPegadoComponentes.setMargins(5, 5, 5, 5);[m
[32m+[m[32m        parametrosPegadoComponentes.weight = 1.0f;[m
[32m+[m
[32m+[m
[32m+[m[32m        linearDerecha.addView(textAngulo, parametrosPegadoComponentes);[m
[32m+[m[32m        linearDerecha.addView(seekBarAngulo, parametrosPegadoComponentes);[m
[32m+[m[32m        linearDerecha.addView(textVelocidad, parametrosPegadoComponentes);[m
[32m+[m[32m        linearDerecha.addView(seekarVelocidadInicial, parametrosPegadoComponentes);[m
[32m+[m[32m        linearDerecha.addView(textAceletacion, parametrosPegadoComponentes);[m
[32m+[m[32m        linearDerecha.addView(seekBarAceleracion, parametrosPegadoComponentes);[m
[32m+[m[32m        linearDerecha.addView(botonEmpezar, parametrosPegadoComponentes);[m
[32m+[m[32m        linearDerecha.addView(botonPausar, parametrosPegadoComponentes);[m
[32m+[m
[32m+[m[32m        return linearPrincipal;[m
[32m+[m
[32m+[m
[32m+[m[32m    }//fin gui[m
[32m+[m
[32m+[m
[32m+[m[32m    private void eventos() {[m
[32m+[m
[32m+[m[32m        //evento botones[m
[32m+[m[32m        botonEmpezar.setOnClickListener(new View.OnClickListener() {[m
[32m+[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m
[32m+[m
[32m+[m[32m                if (botonEmpezar.getText() == "EMPEZAR") {[m
[32m+[m
[32m+[m[32m                    AlmacenDatosRAM.NUEVO=false;[m
[32m+[m[32m                    //para que la bolita deje el trazo de su trayectoria[m
[32m+[m[32m                    //desde su posici√≥n inicial[m
[32m+[m[32m                    i=-1;[m
[32m+[m[32m                    hilo.pausa = false;[m
[32m+[m[32m                    botonEmpezar.setText("NUEVO");[m
[32m+[m[32m                    seekBarAceleracion.setEnabled(false);[m
[32m+[m[32m                    seekBarAngulo.setEnabled(false);[m
[32m+[m[32m                    seekarVelocidadInicial.setEnabled(false);[m
[32m+[m[32m                    botonPausar.setEnabled(true);[m
[32m+[m[32m                    hilo.tiempo = 0;[m
[32m+[m[32m                    botonPausar.setText("PAUSAR");[m
[32m+[m
[32m+[m[32m                } else {[m
[32m+[m
[32m+[m[32m                    AlmacenDatosRAM.NUEVO=true;[m
[32m+[m[32m                    botonEmpezar.setText("EMPEZAR");[m
[32m+[m[32m                    seekBarAceleracion.setEnabled(true);[m
[32m+[m[32m                    seekBarAngulo.setEnabled(true);[m
[32m+[m[32m                    seekarVelocidadInicial.setEnabled(true);[m
[32m+[m[32m                    botonPausar.setEnabled(false);[m
[32m+[m[32m                    estadoInicial();[m
[32m+[m[32m                    hilo.pausa = true;[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m
[32m+[m[32m        //evento del boton activar[m
[32m+[m[32m        botonPausar.setOnClickListener(new View.OnClickListener() {[m
[32m+[m
[32m+[m[32m            public void onClick(View v) {[m
[32m+[m
[32m+[m[32m                if (botonPausar.getText() == "PAUSAR") {[m
[32m+[m
[32m+[m[32m                    botonPausar.setText("CONTINUAR");[m
[32m+[m[32m                    hilo.pausa = true;[m
[32m+[m
[32m+[m[32m                } else {[m
[32m+[m
[32m+[m
[32m+[m[32m                    botonPausar.setText("PAUSAR");[m
[32m+[m[32m                    hilo.pausa = false;[m
[32m+[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m        //eventos seekbar[m
[32m+[m
[32m+[m[32m        // seek_bar_velocidad_inicial[m
[32m+[m[32m        seekarVelocidadInicial.setOnSeekBarChangeListener(new SeekBar.OnSeekBarChangeListener() {[m
[32m+[m[32m            int progressChanged = 0;[m
[32m+[m
[32m+[m[32m            public void onProgressChanged(SeekBar seekBar, int progress, boolean fromUser) {[m
[32m+[m
[32m+[m[32m                int progressChanged = progress;[m
[32m+[m[32m                velocidadInicial = progressChanged;//0 el valor m√≠nimo[m
[32m+[m[32m                //nuevas condiciones iniciales de la escena[m
[32m+[m[32m                estadoInicial();[m
[32m+[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public void onStartTrackingTouch(SeekBar seekBar) {[m
[32m+[m[32m                // TODO Auto-generated method stub[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public void onStopTrackingTouch(SeekBar seekBar) {[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // seek_bar_angulo[m
[32m+[m[32m        seekBarAngulo.setOnSeekBarChangeListener(new SeekBar.OnSeekBarChangeListener() {[m
[32m+[m[32m            int progressChanged = 0;[m
[32m+[m
[32m+[m[32m            public void onProgressChanged(SeekBar seekBar, int progress, boolean fromUser) {[m
[32m+[m[32m                int progressChanged = progress;[m
[32m+[m
[32m+[m[32m                //-90 ser√° el valor m√≠nimo y +90 el valor m√°ximo[m
[32m+[m[32m                angulo = progressChanged -90;[m
[32m+[m
[32m+[m[32m                //condiciones iniciales de la animaci√≥n[m
[32m+[m[32m                estadoInicial();[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public void onStartTrackingTouch(SeekBar seekBar) {[m
[32m+[m[32m                // TODO Auto-generated method stub[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public void onStopTrackingTouch(SeekBar seekBar) {[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m
[32m+[m[32m        // seek_bar_aceleraci√≥n[m
[32m+[m[32m        seekBarAceleracion.setOnSeekBarChangeListener(new SeekBar.OnSeekBarChangeListener() {[m
[32m+[m[32m            int progressChanged = 0;[m
[32m+[m
[32m+[m[32m            public void onProgressChanged(SeekBar seekBar, int progress, boolean fromUser) {[m
[32m+[m[32m                progressChanged = progress;[m
[32m+[m
[32m+[m[32m                //-20 ser√° el valor m√≠nimo y +20 el m√°ximo[m
[32m+[m[32m                aceleracion = (progressChanged) - 20;[m
[32m+[m[32m                estadoInicial();[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public void onStartTrackingTouch(SeekBar seekBar) {[m
[32m+[m[32m                // TODO Auto-generated method stub[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            public void onStopTrackingTouch(SeekBar seekBar) {[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void crearObjetosLaboratorio() {[m
[32m+[m
[32m+[m[32m        //origen en pixeles[m
[32m+[m[32m        float origenX_en_pixeles = CR.pcApxX(10);[m
[32m+[m[32m        float origenY_en_pixeles = CR.pcApxY(50);[m
[32m+[m[32m        //almacenar origen en pixeles[m
[32m+[m[32m        AlmacenDatosRAM.origenX_en_pixeles = origenX_en_pixeles;[m
[32m+[m[32m        AlmacenDatosRAM.origenY_en_pixeles = origenY_en_pixeles;[m
[32m+[m
[32m+[m[32m        //posici√≥n inicial en pixeles[m
[32m+[m[32m        //posici√≥n en pixeles en coordenadas pantalla[m
[32m+[m[32m        float xi_en_pixeles = CR.pcApxX(0);[m
[32m+[m[32m        float yi_en_pixeles = CR.pcApxY(0);[m
[32m+[m[32m        //almacenar en pixeles[m
[32m+[m[32m        AlmacenDatosRAM.xi_en_pixeles = xi_en_pixeles;[m
[32m+[m[32m        AlmacenDatosRAM.yi_en_pixeles = yi_en_pixeles;[m
[32m+[m
[32m+[m[32m        //crear eje x[m
[32m+[m[32m        ejeX = new Flecha(0, 0, CR.pcApxL(30f));[m
[32m+[m[32m        ejeX.setColor(Color.BLACK);[m
[32m+[m[32m        ejeX.setGrosorLinea(CR.pcApxL(0.5f));[m
[32m+[m[32m        objetos[0] = ejeX;[m
[32m+[m
[32m+[m[32m        //crear eje y[m
[32m+[m[32m        ejeY = new Flecha(0, 0, CR.pcApxL(30f));[m
[32m+[m[32m        ejeY.setColor(Color.BLACK);[m
[32m+[m[32m        ejeY.setGrosorLinea(CR.pcApxL(0.5f));[m
[32m+[m[32m        ejeY.rotar(90);[m
[32m+[m[32m        objetos[1] = ejeY;[m
[32m+[m
[32m+[m[32m        //los vectores inicialmente longitud cero[m
[32m+[m[32m        vectAceleracion = new Flecha(xi_en_pixeles, yi_en_pixeles, CR.pcApxL(1.0f));[m
[32m+[m[32m        vectAceleracion.setGrosorLinea(CR.pcApxL(0.5f));[m
[32m+[m[32m         /*[m
[32m+[m[32m         factor de amplificaci√≥n de la longitud del vector[m
[32m+[m[32m         para ver de buen tama√±o el vector[m
[32m+[m[32m         */[m
[32m+[m[32m        float factor_aceleracion = CR.pcApxL(1.5f);[m
[32m+[m[32m        vectAceleracion.setLongitud(factor_aceleracion * AlmacenDatosRAM.aceleracion);[m
[32m+[m[32m        vectAceleracion.setColor(Color.BLUE);[m
[32m+[m[32m        vectAceleracion.rotar(90);[m
[32m+[m[32m        objetos[2] = vectAceleracion;[m
[32m+[m
[32m+[m[32m        //crear velocidad[m
[32m+[m[32m        vectVelocidad = new Flecha(xi_en_pixeles, yi_en_pixeles, CR.pcApxL(1.0f));[m
[32m+[m[32m        vectVelocidad.setGrosorLinea(CR.pcApxL(0.5f));[m
[32m+[m[32m        //float anguloVelocidadGrados = (float) Math.toDegrees(angulo);//AlmacenDatosRAM.anguloVelocidad);[m
[32m+[m[32m        vectVelocidad.rotar(angulo);//anguloVelocidadGrados);[m
[32m+[m[32m         /*[m
[32m+[m[32m         factor de amplificaci√≥n de la longitud del vector[m
[32m+[m[32m         para ver de buen tama√±o el vector[m
[32m+[m[32m         */[m
[32m+[m[32m        float factor_velocidad = CR.pcApxL(0.8f);[m
[32m+[m[32m        vectVelocidad.setLongitud(factor_velocidad * AlmacenDatosRAM.velocidad);[m
[32m+[m[32m        vectVelocidad.setColor(Color.RED);[m
[32m+[m[32m        objetos[3] = vectVelocidad;[m
[32m+[m
[32m+[m[32m        //crear velocidad x[m
[32m+[m[32m        vectVelocidadX = new Flecha(xi_en_pixeles, yi_en_pixeles, CR.pcApxL(1.0f));[m
[32m+[m[32m        vectVelocidadX.setGrosorLinea(CR.pcApxL(0.5f));[m
[32m+[m[32m        vectVelocidadX.setLongitud(factor_velocidad * AlmacenDatosRAM.velocidadX);[m
[32m+[m[32m        vectVelocidadX.setColor(Color.MAGENTA);[m
[32m+[m[32m        objetos[4] = vectVelocidadX;[m
[32m+[m
[32m+[m[32m        //crear velocidad y[m
[32m+[m[32m        vectVelocidadY = new Flecha(xi_en_pixeles, yi_en_pixeles, CR.pcApxL(1.0f));[m
[32m+[m[32m        vectVelocidadY.setGrosorLinea(CR.pcApxL(0.5f));[m
[32m+[m[32m        vectVelocidadY.setLongitud(factor_velocidad * AlmacenDatosRAM.velocidadY);[m
[32m+[m[32m        vectVelocidadY.setColor(Color.rgb(0, 128, 0));[m
[32m+[m[32m        vectVelocidadY.rotar(-90);[m
[32m+[m[32m        objetos[5] = vectVelocidadY;[m
[32m+[m
[32m+[m[32m        //crear bola[m
[32m+[m[32m        bolita = new Particula(xi_en_pixeles, yi_en_pixeles, CR.pcApxL(10.0f));[m
[32m+[m[32m        bolita.setRadio(CR.pcApxL(2f));[m
[32m+[m[32m        bolita.setColor(Color.rgb(200, 0, 0));[m
[32m+[m[32m        objetos[6] = bolita;[m
[32m+[m
[32m+[m[32m        //marcar ejes[m
[32m+[m[32m        marcaEjeX = new Marca("Eje X",  CR.pcApxX(10f),  CR.pcApxY(4f));[m
[32m+[m[32m        marcaEjeX.setTamano(CR.pcApxL(3f));[m
[32m+[m[32m        objetos[7] = marcaEjeX;[m
[32m+[m
[32m+[m[32m        //marcar ejes[m
[32m+[m[32m        marcaEjeY = new Marca("Eje Y",  - CR.pcApxX(2f),  CR.pcApxY(25f));[m
[32m+[m[32m        marcaEjeY.setTamano(CR.pcApxL(3f));[m
[32m+[m[32m        marcaEjeY.rotar(-90f);[m
[32m+[m[32m        objetos[8] = marcaEjeY;[m
[32m+[m
[32m+[m[32m        //estado inicial de la escena[m
[32m+[m[32m        pizarra.setEstadoEscena(objetos);[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void cambiarEstadosEscenaPizarra() {[m
[32m+[m
[32m+[m[32m        //se obtienen los valores de las variables[m
[32m+[m[32m        float x_en_pixeles = AlmacenDatosRAM.x_en_pixeles;[m
[32m+[m[32m        float y_en_pixeles = AlmacenDatosRAM.y_en_pixeles;[m
[32m+[m[32m        float v = AlmacenDatosRAM.velocidad;[m
[32m+[m[32m        float vX = AlmacenDatosRAM.velocidadX;[m
[32m+[m[32m        float vY = AlmacenDatosRAM.velocidadY;[m
[32m+[m[32m        float anguloVelocidad = AlmacenDatosRAM.anguloVelocidad;//en radianes[m
[32m+[m[32m        float a = AlmacenDatosRAM.aceleracion;[m
[32m+[m
[32m+[m[32m        //actualizar la escena[m
[32m+[m[32m        vectVelocidad.setPosicion(x_en_pixeles, y_en_pixeles);[m
[32m+[m[32m        float factor_velocidad = CR.pcApxL(0.8f);[m
[32m+[m[32m        vectVelocidad.setLongitud(factor_velocidad * v);[m
[32m+[m[32m        //el √°ngulo est√° en radianes y se debe convertir a grados[m
[32m+[m[32m        vectVelocidad.rotar((float) Math.toDegrees(anguloVelocidad));[m
[32m+[m
[32m+[m[32m        vectVelocidadX.setLongitud(factor_velocidad * vX);[m
[32m+[m[32m        vectVelocidadX.setPosicion(x_en_pixeles, y_en_pixeles);[m
[32m+[m
[32m+[m[32m        vectVelocidadY.setLongitud(factor_velocidad * vY);[m
[32m+[m[32m        vectVelocidadY.setPosicion(x_en_pixeles, y_en_pixeles);[m
[32m+[m[32m        vectVelocidadY.rotar(90);[m
[32m+[m
[32m+[m[32m        float factor_aceleracion = CR.pcApxL(1.5f);[m
[32m+[m[32m        vectAceleracion.setLongitud(factor_aceleracion * a);[m
[32m+[m[32m        vectAceleracion.setPosicion(x_en_pixeles, y_en_pixeles);[m
[32m+[m
[32m+[m[32m        bolita.setPosicion(x_en_pixeles, y_en_pixeles);[m
[32m+[m[32m        bolita.setColorTrayectoria(Color.rgb(250, 180, 0));[m
[32m+[m[32m        i = i + 1;//necesario para ir dibujando la trayectoria[m
[32m+[m[32m        bolita.setTrayectoria(true, i);[m
[32m+[m[32m        bolita.setGrosorLinea(CR.pcApxL(0.6f));[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void estadoInicial(){[m
[32m+[m
[32m+[m[32m        AlmacenDatosRAM.aceleracion = aceleracion;[m
[32m+[m[32m        AlmacenDatosRAM.angulo = angulo;[m
[32m+[m[32m        AlmacenDatosRAM.vInicial = velocidadInicial;[m
[32m+[m[32m        AlmacenDatosRAM.velocidad= velocidadInicial;[m
[32m+[m[32m        AlmacenDatosRAM.velocidadX = (float)(velocidadInicial*Math.cos(Math.toRadians(angulo)));[m
[32m+[m[32m        AlmacenDatosRAM.velocidadY = (float)(velocidadInicial*Math.sin(Math.toRadians(angulo)));[m
[32m+[m
[32m+[m[32m        float x_i= AlmacenDatosRAM.xi_en_pixeles;[m
[32m+[m[32m        float y_i= AlmacenDatosRAM.yi_en_pixeles;[m
[32m+[m[32m        i=-1;[m
[32m+[m[32m        bolita.borrarDatos();[m
[32m+[m[32m        bolita.setPosicion(x_i, y_i);[m
[32m+[m[32m        vectAceleracion.setPosicion(x_i,y_i);[m
[32m+[m[32m        /*[m
[32m+[m[32m         factor de amplificaci√≥n de la longitud del vector[m
[32m+[m[32m         para ver de buen tama√±o el vector[m
[32m+[m[32m         */[m
[32m+[m[32m        float factor_aceleracion = CR.pcApxL(1.5f);[m
[32m+[m[32m        vectAceleracion.setLongitud(factor_aceleracion*aceleracion);[m
[32m+[m[32m        /*[m
[32m+[m[32m         factor de amplificaci√≥n de la longitud del vector[m
[32m+[m[32m         para ver de buen tama√±o el vector[m
[32m+[m[32m         */[m
[32m+[m[32m        float factor_velocidad = CR.pcApxL(0.8f);[m
[32m+[m[32m        vectVelocidad.setLongitud(factor_velocidad * velocidadInicial);[m
[32m+[m[32m        vectVelocidad.setPosicion(x_i, y_i);[m
[32m+[m[32m        vectVelocidad.rotar(angulo);[m
[32m+[m[32m        //borrar las componentes[m
[32m+[m[32m        vectVelocidadX.setLongitud(0f);[m
[32m+[m[32m        vectVelocidadY.setLongitud(0f);[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m
 }[m
